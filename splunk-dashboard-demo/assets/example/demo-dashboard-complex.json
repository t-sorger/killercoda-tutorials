    "dataSources": {
        "ds_o4jV9wBQ": {
            "type": "ds.search",
            "options": {
                "query": "source=\"/mnt/data/*\" sourcetype=\"_json\"\n| search build_id=$input_build_id$ build_status IN ( $input_build_status$ ) developer=$input_developer$ project=$input_project$ message=$input_message$\n| stats count",
                "queryParameters": {
                    "earliest": "-7d@h",
                    "latest": "now"
                }
            },
            "name": "Builds Today"
        },
        "ds_ahwIRAlQ": {
            "type": "ds.search",
            "options": {
                "query": "source=\"/mnt/data/*\" sourcetype=\"_json\"\n| search build_id=$input_build_id$ build_status IN ( $input_build_status$ ) developer=$input_developer$ project=$input_project$ message=$input_message$\n| bucket _time span=hour\n| stats count by _time",
                "queryParameters": {
                    "earliest": "$global_time.earliest$",
                    "latest": "$global_time.latest$"
                }
            },
            "name": "Builds Over Time"
        },
        "ds_weHA0uKZ": {
            "type": "ds.search",
            "options": {
                "query": "source=\"/mnt/data/*\" sourcetype=\"_json\"\n| search build_id=$input_build_id$ build_status IN ( $input_build_status$ ) developer=$input_developer$ project=$input_project$ message=$input_message$\n| stats count by build_status",
                "queryParameters": {
                    "earliest": "$global_time.earliest$",
                    "latest": "$global_time.latest$"
                }
            },
            "name": "Build Status Distribution"
        },
        "ds_ev01Zsn9": {
            "type": "ds.search",
            "options": {
                "query": "source=\"/mnt/data/*\" sourcetype=\"_json\"\n| search build_id=$input_build_id$ build_status IN ( $input_build_status$ ) developer=$input_developer$ project=$input_project$ message=$input_message$\n| stats count by developer\n| sort -count\n| head 5"
            },
            "name": "Most Frequent Developer"
        },
        "ds_0ipYsaoZ": {
            "type": "ds.search",
            "options": {
                "query": "source=\"/mnt/data/*\" sourcetype=\"_json\"\n| search build_id=$input_build_id$ build_status IN ( $input_build_status$ ) developer=$input_developer$ project=$input_project$ message=$input_message$\n| stats count by project\n| sort -count\n| head 5"
            },
            "name": "Top 5 Most Frequent Project"
        },
        "ds_jyF0WR3f": {
            "type": "ds.search",
            "options": {
                "query": "source=\"/mnt/data/*\" sourcetype=\"_json\"\n| search build_id=$input_build_id$ build_status IN ( $input_build_status$ ) developer=$input_developer$ project=$input_project$ message=$input_message$\n| table time, build_id, build_status, developer, project, message"
            },
            "name": "Builds Overview"
        },
        "ds_9bCDozRA": {
            "type": "ds.search",
            "options": {
                "query": "sourcetype=\"build_error_alert\"\n| stats count",
                "queryParameters": {
                    "earliest": "$global_time.earliest$",
                    "latest": "$global_time.latest$"
                }
            },
            "name": "Error Count"
        }
    },
